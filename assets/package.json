{
    "name": "our-elm-spa-app",
    "version": "1.0.0",
    "description": "A project created with elm-spa",
    "scripts": {
        "start": "npm install && npm run build:dev && npm run dev",
        "test": "elm-test",
        "test:watch": "elm-test --watch",
        "build": "elm-spa build",
        "build:elm": "elm make .elm-spa/defaults/Main.elm --optimize --output='../priv/static/index.html'",
        "build:dev:elm": "elm make src/Main.elm --debug --output=public/dist/elm.compiled.js || true",
        "build:elm-spa": "elm-spa build .",
        "dev:elm": "elm-live src/Main.elm -u -d public -- --debug --output=public/dist/elm.compiled.js",
        "dev:elm-spa": "chokidar src/Pages -c \"elm-spa build .\"",
        "graphql": "elm-graphql http://localhost:4000/api/graphql/v1 --base GetFiveApi --output src/",
        "copy": "copyup \"public/**/*\" ../priv/static/",
        "deploy": "run-s build copy"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "chokidar-cli": "2.1.0",
        "copyfiles": "^2.4.1",
        "elm": "0.19.1-3",
        "elm-live": "4.0.2",
        "elm-spa": "^6.0.4",
        "elm-test": "0.19.1-revision2",
        "elm-tooling": "^1.3.0",
        "npm-run-all": "4.1.5"
    },
    "dependencies": {
        "@dillonkearns/elm-graphql": "^4.0.3"
    }
}
